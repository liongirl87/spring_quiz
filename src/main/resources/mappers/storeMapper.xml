<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.lesson05.DAO.StoreMapper">
 	<select id="selectStore" resultType="com.quiz.lesson05.model.Store">
 		SELECT
 			`id`
			,`name`
			,`phoneNumber`
			,`address`
			,`businessNumber`
			,`introduce`
